<div class="{{className}}">
    {{#if img}}
        <img class="{{classNameImg}}" src="{{img}}" alt="{{alt}}" />
    {{/if}}
    <input
       type="{{type}}"
       value="{{value}}"
       id="{{name}}"
       name="{{name}}"
       class="{{classNameInput}}"
       placeholder="{{placeholder}}"
    />
</div>
